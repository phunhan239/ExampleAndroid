<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dvan.ListView.Aptech.com">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".ListViewActivity"></activity>
        <activity android:name=".ListViewMultiActivity"></activity>
        <activity android:name=".CustomActivity"></activity>
        <activity android:name=".GridViewActivity"></activity>
        <activity android:name=".CustomGridViewActivity"></activity>
    </application>

</manifest>