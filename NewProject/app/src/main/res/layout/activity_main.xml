<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="dvan.newproject.com.newproject.MainActivity">

    <Button
        android:id="@+id/rating_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="goTorating"
        android:text="Rating" />

    <Button
        android:id="@+id/singleChoice_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="goTosingleChoice"
        android:text="Single Choice" />

    <Button
        android:id="@+id/multiChoice"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="goTomultiChoice"
        android:text="Multi Choice" />

    <Button
        android:id="@+id/login_dialog_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="goToLoginDialog"
        android:text="login_dialog" />

    <Button
        android:id="@+id/dt_picker_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="goToDatatimepicker"
        android:text="Date time picker" />

    <Button
        android:id="@+id/waiting_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="WaitingDialog"
        android:text="Waiting Dialog" />
    <Button
        android:id="@+id/download_dialog_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="showDownloadDialog"
        android:text="Download Dialog" />
</LinearLayout>
